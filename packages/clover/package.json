{
	"name": "@poorest/clover",
	"version": "2.1.1",
	"description": "Private npm repository serve",
	"main": "./lib/index.js",
	"files": [
		"lib",
		"docs",
		"i18n",
		"bin",
		"static",
		"views",
		"asset.json",
		"manifest.json",
		"LICENSE",
		"example.yaml"
	],
	"bin": {
		"poorest-npm": "./bin/index"
	},
	"keywords": [
		"private",
		"package",
		"repository",
		"registry",
		"modules",
		"proxy",
		"server"
	],
	"scripts": {
		"test": "yarn jest --forceExit --coverage --verbose --detectOpenHandles",
		"lint": "eslint .",
		"prebuild": "npm run clean",
		"build": "cross-env NODE_ENV=production tsc --build && pnpm run pord",
		"pord": "cross-env NODE_ENV=production node ./lib/build",
		"dev": "node ./lib/build",
		"clean": "rimraf ./lib",
		"watch": "tsc --watch"
	},
